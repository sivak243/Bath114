pipeline {
    agent any

    stages {
        stage('Buid') {
            steps {
                echo 'I am in Build Stage'
                sleep 25
            }
        }
            
         stage('Test') {
            steps {
                echo 'I am in Test Stage'
                sleep 25
            }
         }
            
        stage('Deploy') {
            steps {
                echo 'I am in Deploy Stage'
                sleep 25
            }
            
        }
    
    }
}